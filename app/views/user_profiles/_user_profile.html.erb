<div id="<%= dom_id user_profile %>">
  <p>
    <strong>First name:</strong>
    <%= user_profile.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= user_profile.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user_profile.email %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user_profile.password_digest %>
  </p>

</div>
